	pages = {89--94},
	file = {IEEE Xplore Abstract Record:/Users/hakonknutzen/Library/Application Support/Firefox/Profiles/mf7g6b0m.default/zotero/storage/E4TG24XA/articleDetails.html:text/html;IEEE Xplore Full Text PDF:/Users/hakonknutzen/Library/Application Support/Firefox/Profiles/mf7g6b0m.default/zotero/storage/RH4NS3F7/Chan et al. - 2011 - Constraint-based six degree-of-freedom haptic rend.pdf:application/pdf}
