	author = {Yang, Tae-Heon and Pyo, Dongbum and Kim, Sang-Youn and Cho, Young-Jun and Bae, Yu Dong and Lee, Young Min and Lee, Jeong-Seok and Lee, Eun-Hwa and Kwon, Dong-Soo},
