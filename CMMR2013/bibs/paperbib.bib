	pages = {517--522},
	file = {IEEE Xplore Full Text PDF:/Users/hakonknutzen/Library/Application Support/Firefox/Profiles/mf7g6b0m.default/zotero/storage/C2BBM9MZ/Berkelman og Dzadovsky - 2009 - Extending the motion ranges of magnetic levitation.pdf:application/pdf}
