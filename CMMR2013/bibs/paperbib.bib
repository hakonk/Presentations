	author = {Gonenc, B. and Gurocak, H.},
	year = {2011},
	keywords = {active actuators, actuator input adjustment, Actuators, biological tissues, Bones, controller design, {DC} motors, {DC} servomotor, Force, force control, Force feedback, force profile tracking, haptic interface, Haptic interfaces, Haptics, hybrid actuator, medical computing, medical control systems, multilayered tissue simulation, Muscles, Needles, particle brake, passive actuators, servomotors, stable force response, tissue simulation, virtual needle insertion, virtual needle removal, virtual reality, {VR}},
