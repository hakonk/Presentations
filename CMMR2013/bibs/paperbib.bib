@article{soto-faraco_moving_2004,
	title = {Moving Multisensory Research along: Motion Perception across Sensory Modalities},
	volume = {13},
	copyright = {Copyright © 2004 Association for Psychological Science},
	issn = {0963-7214},
	shorttitle = {Moving Multisensory Research along},
	url = {http://www.jstor.org/stable/20182901},
	doi = {10.2307/20182901},
	abstract = {The past few years have seen a rapid growth of interest regarding how information from the different senses is combined. Historically, the majority of the research on this topic has focused on interactions in the perception of stationary stimuli, but given that the majority of stimuli in the world move, an important question concerns the extent to which principles derived from stationary stimuli also apply to moving stimuli. A key finding emerging from recent work with moving stimuli is that our perception of stimulus movement in one modality is frequently, and unavoidably, modulated by the concurrent movement of stimuli in other sensory modalities. Visual motion has a particularly strong influence on the perception of auditory and tactile motion. These behavioral results are now being complemented by the results of neuroimaging studies that have pointed out the existence of both modality-specific motion-processing areas and areas involved in processing motion in more than one sense. The challenge for the future will be to develop novel experimental paradigms that can integrate behavioral and neuroscientific approaches in order to refine our understanding of multisensory contributions to the perception of movement.},
	number = {1},
	urldate = {2013-04-24},
	journal = {Current Directions in Psychological Science},
	author = {Soto-Faraco, Salvador and Spence, Charles and Lloyd, Donna and Kingstone, Alan},
	month = feb,
	year = {2004},
	note = {{ArticleType:} research-article / Full publication date: Feb., 2004 / Copyright © 2004 Association for Psychological Science},
	pages = {29--32},
	file = {JSTOR Full Text PDF:/Users/hakonknutzen/Library/Application Support/Firefox/Profiles/mf7g6b0m.default/zotero/storage/6HPPM7CU/Soto-Faraco et al. - 2004 - Moving Multisensory Research along Motion Percept.pdf:application/pdf}
},

@inproceedings{kimura_system_2011,
	title = {System development of an admittance-controlled 2-dof haptic device using electrostatic motors},
	doi = {10.1109/WHC.2011.5945496},
	abstract = {This paper describes the system development of an admittance-controlled 2-dof haptic device using electrostatic motors. The unique point of the device is that all components including the motors and sensors are fabricated using non ferromagnetic materials to realize operation in a strong magnetic field. The device consists of an operation handle, two electrostatic motor units and two optical force sensors, and is an improved version of our previous development. The new device has stiffer platforms, improved force sensors, and newly fabricated driving electronics. The employed electrostatic motor is one of the strongest electrostatic actuators and can generate thrust force of several newtons. To operate a motor unit, two three-phase signals are required and they have to be amplified to over 1 {kV.} In our previous development, the signals were amplified by six large commercial amplifiers, which was not practical enough considering the portability of the system. This work developed an original driving electronics for the motor using Field Programmable Gate Array ({FPGA)} and step-up transformers to reduce the system size. The device was implemented on one of the major haptic toolkits, {CHAI} {3D.} To be able to run sample impedance-type programs without any modifications, we implemented our device to behave like impedance-type on {CHAI} {3D.}},
