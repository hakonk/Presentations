@inproceedings{pedrosa_perception_2011,
	title = {Perception of sound renderings via vibrotactile feedback},
	doi = {10.1109/WHC.2011.5945513},
	abstract = {The vibration behavior of acoustic music instruments, as perceived through touch, is known to play an important role in the interaction between performers and music instruments. This research explores the relevance and utility of including a tactile simulation of this behavior in computer music interfaces, in a snapshot taken during early learning, and including tradeoffs of using this channel for other information. Vibrotactile ({VT)} renderings of different types of sounds were presented to subjects via a handle fitted with vibration actuators. Subjects (1) evaluated consistency of the sound with the {VT} rendering, (2) identified from a “lineup” the {VT} rendering that did not match a sound, and (3) provided data on how inclusion of {VT} cues in a rhythm-tapping task affected perception quality of the {VT} rendering. For the last, a distractor task allowed us to measure both the usability of the cues and the degradation in perception of the {VT} rendering. All renderings were either played directly through vibration transducers, or first altered by modifying the sound's frequency content. {VT} playback was delivered at intensities similar to those experienced in traditional acoustic music instruments. Subjects indicated that specific alterations to the original, direct {VT} rendering were consistent with the source sound more often than the original was, and were able to differentiate between correct and incorrect renderings of a source sound. However, the latter ability was masked when subjects were provided with and able to effectively utilize extra {VT} cues added to the {VT} feedback to improve their performance, suggesting that the {VT} cues were of greater utility than the {VT} mimicking. We discuss the relevance of these findings on the design of computer music interfaces.},
	booktitle = {2011 {IEEE} World Haptics Conference ({WHC)}},
	author = {Pedrosa, R. and {MacLean}, {K.E.}},
	year = {2011},
	keywords = {acoustic music instruments, computer music interfaces, Conferences, Decision support systems, Haptic interfaces, music, music computer interfaces, Perception, rendering (computer graphics), rhythm-tapping task, sound rendering, tactile simulation, vibration actuators, vibration behavior, vibration transducers, vibrotactile feedback, vibrotactile rendering},
	pages = {361--366},
	file = {IEEE Xplore Abstract Record:/Users/hakonknutzen/Library/Application Support/Firefox/Profiles/mf7g6b0m.default/zotero/storage/PREWAMEG/articleDetails.html:text/html;IEEE Xplore Full Text PDF:/Users/hakonknutzen/Library/Application Support/Firefox/Profiles/mf7g6b0m.default/zotero/storage/7T9T6ECG/Pedrosa og MacLean - 2011 - Perception of sound renderings via vibrotactile fe.pdf:application/pdf}
},

@article{bianco_measures_2012,
	title = {Measures of Facial Muscle Activation, Intra-oral Pressure and Mouthpiece Force in Trumpet Playing},
	volume = {41},
	issn = {0929-8215},
	url = {http://www.tandfonline.com/doi/abs/10.1080/09298215.2011.646278},
	doi = {10.1080/09298215.2011.646278},
	abstract = {Abstract Trumpet performance builds on the synergy of multiple factors. From a physiological point of view, the production of a simple musical tone relies on the control of the pressure at the lips, the contact force on the mouthpiece, and the oral articulatory gestures and configurations. Whereas previous studies described the magnitude of these control parameters in the quasi-static regime of the oscillation, the present investigation focuses on their temporal evolution and interrelation. Intra-oral pressure, mouthpiece force, facial muscular activity of two groups of muscles, and the radiated sound were recorded from three professional players performing isolated and articulated tones with three different dynamics. Results showed that the experimental conditions affected the amount and location of variability in the performance, and the behaviour of intra-oral pressure at note initiation. All together, this suggests that different dynamics and sequencing may cause a reorganization of the mechanisms underlying the production of a tone.},
	number = {1},
	urldate = {2013-04-18},
	journal = {Journal of New Music Research},
	author = {Bianco, Tommaso and Freour, Vincent and Cossette, Isabelle and Bevilacqua, Frédéric and Caussé, René},
