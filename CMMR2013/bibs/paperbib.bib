	pages = {553--558},
	file = {IEEE Xplore Abstract Record:/Users/hakonknutzen/Library/Application Support/Firefox/Profiles/mf7g6b0m.default/zotero/storage/A7V4VUQ5/articleDetails.html:text/html;IEEE Xplore Full Text PDF:/Users/hakonknutzen/Library/Application Support/Firefox/Profiles/mf7g6b0m.default/zotero/storage/VK76KA7F/Li og Constantinescu - 2009 - Remote dynamic proxies for wave-based peer-to-peer.pdf:application/pdf}
