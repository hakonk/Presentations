	year = {2011},
	keywords = {bidirectionality, convexity detection, human tactile perception, Humans, microphones, rough surfaces, roughness evaluations, self-reference, surface roughness, tactile sensation evaluation, tactile sensing, tactile sensor, tactile sensors},
