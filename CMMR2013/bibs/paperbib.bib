@inproceedings{okazaki_tactile_2011,
	title = {Tactile enhancement structure mimicking hair follicle receptors},
	doi = {10.1109/WHC.2011.5945508},
	abstract = {We propose a tactile enhancement structure inspired by hair follicle receptors. Unlike other receptors, part of the hair follicle receptor is exposed to the outside. Recent research has shown that skin hair contributes to perception of minute forces that cannot be perceived with glabrous skin. We considered how the skin perceives these minute forces. Our tactile enhancement device mimics the structure of hair follicle receptors. A matrix structure simulating artificial body hair is driven by minute forces external to the skin surface. This structure can be used on any surface of the human body because it is fully composed of passive elements.},
