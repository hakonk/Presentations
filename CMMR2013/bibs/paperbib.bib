@incollection{george_combining_2012,
	series = {Lecture Notes in Computer Science},
	title = {Combining Brain-Computer Interfaces and Haptics: Detecting Mental Workload to Adapt Haptic Assistance},
	copyright = {©2012 Springer-Verlag Berlin Heidelberg},
	isbn = {978-3-642-31400-1, 978-3-642-31401-8},
	shorttitle = {Combining Brain-Computer Interfaces and Haptics},
	url = {http://link.springer.com/chapter/10.1007/978-3-642-31401-8_12},
	abstract = {In this paper we introduce the combined use of Brain-Computer Interfaces ({BCI)} and Haptic interfaces. We propose to adapt haptic guides based on the mental activity measured by a {BCI} system. This novel approach is illustrated within a proof-of-concept system: haptic guides are toggled during a path-following task thanks to a mental workload index provided by a {BCI.} The aim of this system is to provide haptic assistance only when the user’s brain activity reflects a high mental workload. A user study conducted with 8 participants shows that our proof-of-concept is operational and exploitable. Results show that activation of haptic guides occurs in the most difficult part of the path-following task. Moreover it allows to increase task performance by 53\% by activating assistance only 59\% of the time. Taken together, these results suggest that {BCI} could be used to determine when the user needs assistance during haptic interaction and to enable haptic guides accordingly.},
	number = {7282},
	urldate = {2013-04-18},
	booktitle = {Haptics: Perception, Devices, Mobility, and Communication},
	publisher = {Springer Berlin Heidelberg},
	author = {George, Laurent and Marchal, Maud and Glondu, Loeiz and Lécuyer, Anatole},
	editor = {Isokoski, Poika and Springare, Jukka},
	month = jan,
	year = {2012},
	keywords = {Adaptation, Brain-Computer Interface, Computer Imaging, Vision, Pattern Recognition and Graphics, Computers and Education, Computers and Society, {EEG}, Force-Feedback, Guidance, Information Systems Applications (incl. Internet), Mental Workload, Multimedia Information Systems, User Interfaces and Human Computer Interaction},
	pages = {124--135},
	file = {Full Text PDF:/Users/hakonknutzen/Library/Application Support/Firefox/Profiles/mf7g6b0m.default/zotero/storage/HKF6AWDM/George et al. - 2012 - Combining Brain-Computer Interfaces and Haptics D.pdf:application/pdf;Snapshot:/Users/hakonknutzen/Library/Application Support/Firefox/Profiles/mf7g6b0m.default/zotero/storage/KDFEXVQC/978-3-642-31401-8_12.html:text/html}
