	pages = {95--100},
	file = {IEEE Xplore Abstract Record:/Users/hakonknutzen/Library/Application Support/Firefox/Profiles/mf7g6b0m.default/zotero/storage/BEXNMQNN/articleDetails.html:text/html;IEEE Xplore Full Text PDF:/Users/hakonknutzen/Library/Application Support/Firefox/Profiles/mf7g6b0m.default/zotero/storage/5II84TJS/Yang et al. - 2011 - A new subminiature impact actuator for mobile devi.pdf:application/pdf}
