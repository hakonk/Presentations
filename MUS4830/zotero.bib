	pages = {417--421},
	file = {IEEE Xplore Abstract Record:/Users/hakonknutzen/Library/Application Support/Firefox/Profiles/mf7g6b0m.default/zotero/storage/4E2A4TFE/articleDetails.html:text/html;IEEE Xplore Full Text PDF:/Users/hakonknutzen/Library/Application Support/Firefox/Profiles/mf7g6b0m.default/zotero/storage/VMN4BBT8/Plaisier et al. - 2009 - The role of item fixation in haptic search.pdf:application/pdf}
