	file = {Full Text PDF:/Users/hakonknutzen/Library/Application Support/Firefox/Profiles/mf7g6b0m.default/zotero/storage/4TXX9NKV/Helbig og Ernst - 2008 - Haptic perception in interaction with other senses.pdf:application/pdf;Snapshot:/Users/hakonknutzen/Library/Application Support/Firefox/Profiles/mf7g6b0m.default/zotero/storage/MMJE9WGR/978-3-7643-7612-3_18.html:text/html}
