	pages = {263--268},
	file = {IEEE Xplore Abstract Record:/Users/hakonknutzen/Library/Application Support/Firefox/Profiles/mf7g6b0m.default/zotero/storage/KSN8VR7B/articleDetails.html:text/html;IEEE Xplore Full Text PDF:/Users/hakonknutzen/Library/Application Support/Firefox/Profiles/mf7g6b0m.default/zotero/storage/QKFSGJQ9/Kimura et al. - 2011 - System development of an admittance-controlled 2-d.pdf:application/pdf}
