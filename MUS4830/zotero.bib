@inproceedings{adams_tactile_2011,
	title = {Tactile data entry for extravehicular activity},
	doi = {10.1109/WHC.2011.5945503},
	abstract = {In the task-saturated environment of extravehicular activity ({EVA)}, an astronaut's ability to leverage suit-integrated information systems is limited by a lack of options for data entry. In particular, bulky gloves inhibit the ability to interact with standard computing interfaces such as a mouse or keyboard. This paper presents the results of a preliminary investigation into a system that permits the space suit gloves themselves to be used as data entry devices. Hand motion tracking is combined with simple finger gesture recognition to enable use of a virtual keyboard, while tactile feedback provides touch-based context to the graphical user interface ({GUI)} and positive confirmation of keystroke events. In human subject trials, conducted with twenty participants using a prototype system, participants entered text significantly faster with tactile feedback than without (p = 0.02). The results support incorporation of vibrotactile information in a future system that will enable full touch typing and general mouse interactions using instrumented {EVA} gloves.},
	booktitle = {2011 {IEEE} World Haptics Conference ({WHC)}},
	author = {Adams, {R.J.} and Olowin, {A.B.} and Hannaford, B. and Sands, {O.S.}},
	year = {2011},
	keywords = {Accelerometers, astronaut ability, data entry performance, extravehicular activity, finger gesture recognition, Fingers, gesture recognition, graphical user interface, graphical user interfaces, hand motion tracking, Haptic interfaces, human computer interaction, Humans, instrumented {EVA} gloves, Keyboards, {NASA}, space suit gloves, standard computing interface, suit integrated information system, tactile data entry, Tactile feedback, tactile sensors, target tracking, task saturated environment, touch based context, vibrotactile information, virtual keyboard},
	pages = {305--310},
	file = {IEEE Xplore Abstract Record:/Users/hakonknutzen/Library/Application Support/Firefox/Profiles/mf7g6b0m.default/zotero/storage/RACFKNXU/articleDetails.html:text/html;IEEE Xplore Full Text PDF:/Users/hakonknutzen/Library/Application Support/Firefox/Profiles/mf7g6b0m.default/zotero/storage/FXANGCKH/Adams et al. - 2011 - Tactile data entry for extravehicular activity.pdf:application/pdf}
},

@inproceedings{antolini_haptic_2011,
	title = {A haptic direction indicator using the gyro effect},
	doi = {10.1109/WHC.2011.5945494},
	abstract = {The preliminary study described in this paper is about the assessment of the control parameters of a non-grounded haptic device giving kinaesthetic stimuli to indicate to the user a direction to follow to reach a target location. The haptic sensation is created by tilting one or more rotating flywheels along an axis, controlling the direction and the amount of tilt, the velocity of the tilt (during the pulse and the repositioning of the flywheel), the frequency of pulses. The current prototype has only one flywheel and is able to tilt along a single axis, thus indicating only “right” or “left” direction. The experiments performed with the prototype allowed us to define a set of requirements (torque produced, duration of the stimuli) for a more complex prototype (2 flywheels and 2 {DOF)} to be developed and tested as future work.},
	booktitle = {2011 {IEEE} World Haptics Conference ({WHC)}},
	author = {Antolini, M. and Bordegoni, M. and Cugini, U.},
	year = {2011},
	keywords = {aerospace computing, Angular velocity, augmented reality, Brushless motors, complex prototype, direction control, Equations, flywheel rotation, flywheels, gyro effect, gyroscopes, haptic direction indicator, Haptic interfaces, haptic sensation, kinaesthetic stimuli, mobile handsets, nongrounded haptic device, Prototypes, radio direction-finding, target location, tilt velocity, Torque},
