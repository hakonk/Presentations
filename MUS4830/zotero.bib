	keywords = {auditory-tactile integration, Computers and Education, Computers and Society, haptic stiffness, Information Systems Applications (incl. Internet), loudness perception, Models and Principles, Multimedia Information Systems, User Interfaces and Human Computer Interaction},
	pages = {79--88},
