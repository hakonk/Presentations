@article{lewkowicz_emergence_2009,
	title = {The emergence of multisensory systems through perceptual narrowing},
	volume = {13},
	issn = {1364-6613},
	url = {http://www.sciencedirect.com/science/article/pii/S1364661309001740},
	doi = {10.1016/j.tics.2009.08.004},
	abstract = {According to conventional wisdom, multisensory development is a progressive process that results in the growth and proliferation of perceptual skills. We review new findings indicating that a regressive process – perceptual narrowing – also contributes in critical ways to perceptual development. These new data reveal that young infants are able to integrate non-native faces and vocalizations, that this broad multisensory perceptual tuning is present at birth, and that this tuning narrows by the end of the first year of life, leaving infants with the ability to integrate only socio-ecologically-relevant multisensory signals. This narrowing process forces us to reconsider the traditional progressive theories of multisensory development and opens up several new evolutionary questions as well.},
	number = {11},
	urldate = {2013-07-01},
	journal = {Trends in Cognitive Sciences},
	author = {Lewkowicz, David J. and Ghazanfar, Asif A.},
	month = nov,
	year = {2009},
