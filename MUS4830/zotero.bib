	pages = {646--651},
	file = {IEEE Xplore Abstract Record:/Users/hakonknutzen/Library/Application Support/Firefox/Profiles/mf7g6b0m.default/zotero/storage/5BV7CZAJ/articleDetails.html:text/html;IEEE Xplore Full Text PDF:/Users/hakonknutzen/Library/Application Support/Firefox/Profiles/mf7g6b0m.default/zotero/storage/IHTMD495/Forrest et al. - 2009 - Haptic stiffness identification by veterinarians a.pdf:application/pdf}
