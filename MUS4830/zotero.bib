	pages = {58--66},
	file = {Full Text PDF:/Users/hakonknutzen/Library/Application Support/Firefox/Profiles/mf7g6b0m.default/zotero/storage/VJMP7Q7F/Bellan et al. - 2012 - Evidence for ‘Visual Enhancement of Touch’ Mediate.pdf:application/pdf;Snapshot:/Users/hakonknutzen/Library/Application Support/Firefox/Profiles/mf7g6b0m.default/zotero/storage/RSHI95HD/978-3-642-31401-8_6.html:text/html}
