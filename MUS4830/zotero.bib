	publisher = {Springer Berlin Heidelberg},
	month = jan,
	year = {2007},
	keywords = {Computer Appl. in Arts and Humanities, computer graphics, Information Storage and Retrieval, Multimedia Information Systems},
	pages = {169--183},
	file = {Full Text PDF:/Users/hakonknutzen/Library/Application Support/Firefox/Profiles/mf7g6b0m.default/zotero/storage/IEH5E9ER/2007 - SyncPlayer An Advanced Audio Player.pdf:application/pdf;Snapshot:/Users/hakonknutzen/Library/Application Support/Firefox/Profiles/mf7g6b0m.default/zotero/storage/VCMNIUW3/978-3-540-74048-3_8.html:text/html}
