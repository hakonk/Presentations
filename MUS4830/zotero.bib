	file = {Snapshot:/Users/hakonknutzen/Library/Application Support/Firefox/Profiles/mf7g6b0m.default/zotero/storage/XQEII3X5/abstract;jsessionid=71D2A3B8DC0F501F81214E71CBF6974C.html:text/html}
},

@inproceedings{garre_interactive_2011,
