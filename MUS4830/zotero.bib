@inproceedings{braasch_telehaptic_2013,
	address = {Montreal, Canada},
	title = {Telehaptic interfaces for interpersonal communication within a music ensemble},
	volume = {19},
	copyright = {© 2013 Acoustical Society of America},
	url = {http://link.aip.org/link/?PMA/19/015027/1},
	doi = {10.1121/1.4800956},
	abstract = {Visual communication is an important aspect of musical performance. It is used, for example, to pick up temporal cues and find the right entries. Visual cues can also be instrumental to negotiating the solo order in improvised music or enabling social exchanges, such as signalizing to someone that her solo was well received. The problem with visual communication is that one has to catch someone else's attention. Visual cues outside someone's visual field cannot be detected, even more so if the addressee is busy reading a music score or closing his eyes in a Free Music session. While acoustic communication avoids these challenges, it tends to interfere with the music. The haptic modality has the advantage that it does not necessarily interfere with the acoustic signal, and does not require attention. However, it allows interpersonal communication if both parties are within close proximity. Using telematic interfaces the problem of proximity can be solved to allow participants to communicate over any physical distance. In this paper, our research focuses on polyphonic pitch model to transform audio signals into haptic cues for hearing impaired users.},
	urldate = {2013-07-24},
	booktitle = {{ICA} 2013 Montreal},
	publisher = {{ASA}},
	author = {Braasch, Jonas and Nort, Doug Van and Oliveros, Pauline and Krueger, Ted},
	year = {2013},
	pages = {015027},
	file = {Full Text PDF:/Users/hakonknutzen/Library/Application Support/Firefox/Profiles/mf7g6b0m.default/zotero/storage/HZ464Q9D/Braasch et al. - 2013 - Telehaptic interfaces for interpersonal communicat.pdf:application/pdf;Snapshot:/Users/hakonknutzen/Library/Application Support/Firefox/Profiles/mf7g6b0m.default/zotero/storage/54DJ26IJ/p015027_s1.html:text/html}
},

