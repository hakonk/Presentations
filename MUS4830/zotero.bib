	pages = {422--427},
	file = {IEEE Xplore Abstract Record:/Users/hakonknutzen/Library/Application Support/Firefox/Profiles/mf7g6b0m.default/zotero/storage/Z8IED7H5/articleDetails.html:text/html;IEEE Xplore Full Text PDF:/Users/hakonknutzen/Library/Application Support/Firefox/Profiles/mf7g6b0m.default/zotero/storage/P57UBA6B/Garre og Otaduy - 2009 - Haptic rendering of complex deformations through h.pdf:application/pdf}
