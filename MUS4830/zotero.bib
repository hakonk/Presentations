@inproceedings{lee_stability_2011,
	title = {Stability of internet-based teleoperation systems using Bayesian predictions},
	doi = {10.1109/WHC.2011.5945536},
	abstract = {In this paper, we present a Bayesian prediction approach to improve stability of teleoperation systems over the Internet. Motion and force data flows in a teleoperation system are formulated in discrete time state-space models predicted by Bayesian filters, including the Kalman filter and particle filter. The particle filter, which is known as a robust tracking method in nonlinear and non-Gaussian environments, is used to compensate for the time-varying Internet delay. A stochastic analysis is presented to show stability improvement of a teleoperation system in the case when convergence of a Bayesian predictor is achieved and a generalized form of scattering transformation is used as a control scheme. Experiments are performed using a teleoperation system based on virtual reality. A haptic device is used as a human operator in conjunction with a mechanic-based virtual teleoperator by implementing the proposed Bayesian prediction method. Experimental results show that the proposed method improve stability of an overall teleoperation system in the presence of time-varying delay.},
	booktitle = {2011 {IEEE} World Haptics Conference ({WHC)}},
	author = {Lee, Jae-young and Payandeh, S.},
	year = {2011},
	keywords = {Bayes methods, Bayesian, Bayesian methods, Bayesian predictions, Delay, delays, discrete time filters, discrete time state-space models, Force, Gaussian processes, Haptic Device, haptic interface, Haptic interfaces, human operator, human-robot interaction, Internet, Internet-Based teleoperation systems, Kalman filter, Kalman filters, mechanic-based virtual teleoperator, {nonGaussian} environments, particle filter, particle filtering (numerical methods), Particle filters, predictive control, Scattering, scattering transformation, sequential Monte Carlo, stability, Stability analysis, state-space methods, stochastic analysis, Teleoperation, telerobotics, time-varying delay, time-varying filters, time-varying Internet delay, virtual reality},
	pages = {499--504},
	file = {IEEE Xplore Abstract Record:/Users/hakonknutzen/Library/Application Support/Firefox/Profiles/mf7g6b0m.default/zotero/storage/RQP63MW7/articleDetails.html:text/html;IEEE Xplore Full Text PDF:/Users/hakonknutzen/Library/Application Support/Firefox/Profiles/mf7g6b0m.default/zotero/storage/B6VMGFUD/Lee og Payandeh - 2011 - Stability of internet-based teleoperation systems .pdf:application/pdf}
