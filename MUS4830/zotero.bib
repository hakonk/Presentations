	pages = {487--492},
	file = {IEEE Xplore Abstract Record:/Users/hakonknutzen/Library/Application Support/Firefox/Profiles/mf7g6b0m.default/zotero/storage/ZSDV2NGP/articleDetails.html:text/html;IEEE Xplore Full Text PDF:/Users/hakonknutzen/Library/Application Support/Firefox/Profiles/mf7g6b0m.default/zotero/storage/XSK42ZJE/Jazayeri og Tavakoli - 2011 - A passivity criterion for sampled-data bilateral t.pdf:application/pdf}
},

@inproceedings{weinberg_variable_2009,
