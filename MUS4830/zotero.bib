	editor = {Magnusson, Charlotte and Szymczak, Delphine and Brewster, Stephen},
	month = jan,
	year = {2012},
	keywords = {auditory-haptic interaction, Computers and Education, Computers and Society, Household appliances, impulsive sounds, Media Design, Models and Principles, Multimedia Information Systems, product sound quality, User Interfaces and Human Computer Interaction, vibration of plates},
	pages = {121--130},
	file = {Full Text PDF:/Users/hakonknutzen/Library/Application Support/Firefox/Profiles/mf7g6b0m.default/zotero/storage/WA8TICNX/Altinsoy - 2012 - Knocking Sound as Quality Sign for Household Appli.pdf:application/pdf;Snapshot:/Users/hakonknutzen/Library/Application Support/Firefox/Profiles/mf7g6b0m.default/zotero/storage/DVIPA5VE/978-3-642-32796-4_13.html:text/html}
