	pages = {1815--1821},
	file = {IEEE Xplore Abstract Record:/Users/hakonknutzen/Library/Application Support/Firefox/Profiles/mf7g6b0m.default/zotero/storage/X32JZIEA/abs_all.html:text/html;IEEE Xplore Full Text PDF:/Users/hakonknutzen/Library/Application Support/Firefox/Profiles/mf7g6b0m.default/zotero/storage/PZUKH39J/Romano et al. - 2010 - Automatic filter design for synthesis of haptic te.pdf:application/pdf}
