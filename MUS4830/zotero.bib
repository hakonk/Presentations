	keywords = {Compliance, Computer Imaging, Vision, Pattern Recognition and Graphics, Computers and Education, Computers and Society, Information Systems Applications (incl. Internet), Multimedia Information Systems, Proprioception, Prosthetics, Sensory Substitution, User Interfaces and Human Computer Interaction},
