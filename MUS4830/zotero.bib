@incollection{gentaz_haptic_2008,
	title = {Haptic perceptual illusions},
	copyright = {©2008 Birkhäuser Basel},
	isbn = {978-3-7643-7611-6, 978-3-7643-7612-3},
	url = {http://link.springer.com/chapter/10.1007/978-3-7643-7612-3_17},
	abstract = {Perceptual illusions refer to systematically oriented errors in the perception of figures or scenes, and these errors are observed in almost all people. For centuries, they have been called ‘opto-geometric illusions’ because it was thought that they concerned only visual perception. Although visual errors are relatively rare, comments and questions about visual illusions are found as early as in the Greek and Roman literature. In scientific psychology, the theoretical and practical problems raised by these deformations have been intensively studied since the end of the 19th Century, and the elements of the figure inducing each error are now identified. However, there is no general theory explaining all the visual illusions. Instead, each figure must be analysed in order to determine the specific processes leading to the error.},
	language = {en},
	urldate = {2013-06-21},
	booktitle = {Human Haptic Perception: Basics and Applications},
