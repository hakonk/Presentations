	pages = {208--213},
	file = {IEEE Xplore Abstract Record:/Users/hakonknutzen/Library/Application Support/Firefox/Profiles/mf7g6b0m.default/zotero/storage/WZP5G4UH/articleDetails.html:text/html;IEEE Xplore Full Text PDF:/Users/hakonknutzen/Library/Application Support/Firefox/Profiles/mf7g6b0m.default/zotero/storage/E8KZIDDI/Vittorias et al. - 2009 - Perceptual coding of haptic data in time-delayed t.pdf:application/pdf}
