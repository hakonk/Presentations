@article{rasamimanana_towards_2012,
	title = {Towards a Conceptual Framework for Exploring and Modelling Expressive Musical Gestures},
	volume = {41},
	issn = {0929-8215},
	url = {http://www.tandfonline.com/doi/abs/10.1080/09298215.2011.642391},
	doi = {10.1080/09298215.2011.642391},
	abstract = {Abstract This paper reports on a conceptual framework for the analysis and modelling of expressive musical gestures. This framework, called space of possibilities ({SoP)}, is based on crossing the different constraints a player must face when playing his/her instrument. These constraints can be acoustical, biomechanical, and are considered along with the musicians' acquired skills. Such an approach can provide a unified ground for modelling performer–instrument relationships by encompassing different perspectives within a single view. We propose to define this {SoP} through the review of various computational techniques for expressive gestures: feature-based approaches, dynamic modelling, and segmental modelling. More precisely, we see how the notions of gesture invariance and variability, gesture qualities, and gesture primitives can help structure {SoP.}},
