@inproceedings{wang_analysis_2011,
	title = {Analysis on increasing transparency for penalty-based six degree-of-freedom haptic rendering},
	doi = {10.1109/WHC.2011.5945466},
	abstract = {Virtual coupling is commonly used to maintain stability of penalty-based haptic rendering. However, due to dozens of design parameters involved in the dynamics and force model, to increase transparency while maintaining stability using virtual coupling remains a challenge for 6-Degree-of-Freedom ({DOF)} haptic rendering. Using an equivalent-angle analysis, we transform 3-{DOF} rotation into 1-{DOF} rotation, thereby transforming the 6-{DOF} dynamics into six 1-{DOF} dynamics. For each 1-{DOF} dynamics, a damped vibration model is proposed to analyze the computation of the `graphic tool'. The equilibrium position/angle is defined to analyze the convergence of numerical integration. With this approach, we can improve the transparency of 6-{DOF} haptic rendering in both free space and constraint space by experimentally determining only two parameters: the integration time step and the number of integrations in each simulation loop. Using this method, we can improve the coupling stiffness to fully exploit the display capability of the haptic device and maintain the stability of the haptic rendering.},
	booktitle = {2011 {IEEE} World Haptics Conference ({WHC)}},
	author = {Wang, Zhixiang and Wang, Dangxiao and Zhang, Yuru and Lin, {M.C.}},
	year = {2011},
	keywords = {1-degree-of-freedom rotation, 3-degree-of-freedom rotation, 6-degree-of-freedom dynamics, 6-degree-of-freedom haptic rendering, coupling stiffness, Couplings, damped vibration model, design, design parameter, Dynamics, equivalent-angle analysis, force model, graphic tool, Haptic interfaces, integration, integration step, Mathematical model, numerical integration, penalty-based haptic rendering, rendering (computer graphics), Six Degree-of-Freedom Haptic Rendering, stability maintenance, vibrations, virtual coupling},
	pages = {83--88},
	file = {IEEE Xplore Abstract Record:/Users/hakonknutzen/Library/Application Support/Firefox/Profiles/mf7g6b0m.default/zotero/storage/PVU3E5PB/articleDetails.html:text/html;IEEE Xplore Full Text PDF:/Users/hakonknutzen/Library/Application Support/Firefox/Profiles/mf7g6b0m.default/zotero/storage/S3V4NMKX/Wang et al. - 2011 - Analysis on increasing transparency for penalty-ba.pdf:application/pdf}
