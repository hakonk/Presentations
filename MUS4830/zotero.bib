	pages = {166--171},
	file = {IEEE Xplore Full Text PDF:/Users/hakonknutzen/Library/Application Support/Firefox/Profiles/mf7g6b0m.default/zotero/storage/FF6MX2KU/Ballesteros et al. - 2009 - ERP evidence of tactile texture processing Effect.pdf:application/pdf}
