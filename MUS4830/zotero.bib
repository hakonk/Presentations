	file = {IEEE Xplore Full Text PDF:/Users/hakonknutzen/Library/Application Support/Firefox/Profiles/mf7g6b0m.default/zotero/storage/9G82CZIG/Yamauchi et al. - 2009 - Virtual active touch Perceived roughness through .pdf:application/pdf}
