@article{marsden_interrogating_2012,
	title = {Interrogating Melodic Similarity: A Definitive Phenomenon or the Product of Interpretation?},
	volume = {41},
	issn = {0929-8215},
	shorttitle = {Interrogating Melodic Similarity},
	url = {http://www.tandfonline.com/doi/abs/10.1080/09298215.2012.740051},
	doi = {10.1080/09298215.2012.740051},
	abstract = {Abstract The nature of melodic similarity is interrogated through a survey of the different means by which the phenomenon has been studied, examination of methods for measuring melodic similarity, a Monte Carlo analysis of data from the experiment which formed the basis for the ‘ground truth’ used in the {MIREX} 2005 contest on melodic similarity, and examples of interest in the music of Mozart. Melodic similarity has been studied by a number of means, sometimes quite contrasting, which lead to important differences in the light of the finding that similarity is dependent on context. Models of melodic similarity based on reduction show that the existence of multiple possible reductions forms a natural basis for similarity to depend on interpretation. Examination of the {MIREX} 2005 data shows wide variations in subjects' judgements of melodic similarity and some evidence that the perceived similarity between two melodies can be influenced by the presence of a third melody. Examples from Mozart suggest that he deliberately exploited the possibilities inherent in recognizing similarity through different interpretations. It is therefore proposed that similarity be thought of not as a distinct and definite function of two melodies but as something created in the minds of those who hear the melodies.},
	number = {4},
	urldate = {2013-04-18},
	journal = {Journal of New Music Research},
	author = {Marsden, Alan},
	year = {2012},
	pages = {323--335},
	file = {Full Text PDF:/Users/hakonknutzen/Library/Application Support/Firefox/Profiles/mf7g6b0m.default/zotero/storage/B8HG9ZP9/Marsden - 2012 - Interrogating Melodic Similarity A Definitive Phe.pdf:application/pdf;Snapshot:/Users/hakonknutzen/Library/Application Support/Firefox/Profiles/mf7g6b0m.default/zotero/storage/5ZF3KHJ7/09298215.2012.html:text/html}
},

